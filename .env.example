# JaytiPargal.in Environment Configuration
# Copy this file to .env and fill in your values

# Django Settings
DEBUG=False
SECRET_KEY=your-secret-key-here
ALLOWED_HOSTS=localhost,127.0.0.1,jaytipargal.in

# Database - PostgreSQL for production
DATABASE_URL=postgresql://user:password@localhost:5432/jaytipargal
# For local development, comment above and use SQLite (default)

# Gemini API Key for AI Goal Generation
GEMINI_API_KEY=your-gemini-api-key-here

# Time Zone
TIME_ZONE=Asia/Kolkata

# Birth Date Configuration (Feb 6)
BIRTH_DATE_DAY=6
BIRTH_DATE_MONTH=2
